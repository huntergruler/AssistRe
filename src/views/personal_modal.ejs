<div class="modal fade" id="personalModal" tabindex="-1" aria-labelledby="personalModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="personalModalLabel">Personal Info</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="modalcontainer">
          <div class="col-md-12">
            <div class="form-container">
              <form id="profile-group">
                <div class="form-row ">
                  <label class="col-md-4 label-above" style="font-size: 14px;">First</label>
                  <label class="col-md-3 label-above" style="font-size: 14px;">Last</label>
                </div>
                <div class="form-row ">
                  <label for="firstName" class="col-md-3 col-form-label justify-content-right">Name</label>
                  <input type="text" class="form-control-sm col-md-3" placeholder="First Name" id="firstName" name="firstName" required>
                  <input type="text" class="form-control-sm col-md-4" placeholder="Last Name" id="lastName" name="lastName" required>
                </div>
                <div class="form-row">
                  <label for="address" class="col-md-3 col-form-label">Address</label>
                  <input type="text" class="form-control-sm col-md-7" placeholder="Address" id="address" name="address" required>
                </div>
                <div class="form-row displaytext">
                  <label for="city" class="col-md-3 view-mode-r">City, State</label>
                  <div class="col-md-4 view-mode-l" id="cityState"></div>
                </div>
                <div class="form-row">
                  <label for="zip" class="col-md-3 col-form-label">Zip</label>
                  <input type="text" class="form-control-sm col-md-3" placeholder="Zip Code" id="zip" name="zip" required onmouseleave="lookupCityState()" onblur="lookupCityState()">
                </div>
                <div class="form-row">
                  <label for="phoneNumber" class="col-md-3 col-form-label">Phone #</label>
                  <input type="text" class="form-control-sm col-md-4" placeholder="Phone Number" id="phoneNumber" name="phoneNumber" required>
                </div>
                <input type="hidden" id="userid" name="userid">
                <div class="form-row button-row">
                  <div class="modal-footer">
                    <button type="button" id="modify-button" class="modify-button" data-dismiss="modal" onclick="savePersonalChanges()">Save</button>
                    <button type="button" id="modify-button" class="modify-button" data-dismiss="modal">Cancel</button>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>